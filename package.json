{
  "name": "node-js-getting-started",
  "version": "0.2.6",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "6.10.2"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev supervisor -w server,app.js -e node,js,html app",
    "build": "webpack",
    "production": "webpack --config webpack.production.config.js -p & cross-env NODE_ENV=production node app",
    "start": "webpack --config webpack.production.config.js -p --progress & cross-env NODE_ENV=production node app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "dependencies": {
    "@google/maps": "^0.4.3",
    "axios": "^0.16.2",
    "bootstrap-vue": "^0.16.1",
    "consolidate": "^0.14.5",
    "ejs": "2.5.6",
    "express": "latest",
    "pg": "latest",
    "quill": "^1.2.5",
    "vue": "^2.3.3",
    "vue-quill-editor": "^2.2.1",
    "vue-resource": "^1.3.3",
    "vue-router": "^2.5.3",
    "vue-scroll": "^2.0.3",
    "vue2-google-maps": "^0.8.0",
    "vuex": "^2.3.1",
    "webpack": "^2.6.1"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
